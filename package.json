{
  "name": "japani",
  "version": "1.0.0",
  "description": "QXBroker Nison-based Binary Options Bot",
  "type": "module",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prepare": "echo 'Skipping playwright install in production'",
    "postinstall": "npx playwright install chromium --with-deps || npx playwright install chromium",
    "collect:screenshot": "node src/collector/collect_screenshot.js",
    "collect:candles": "node src/collector/collect_screenshot.js --extract-candles",
    "signals:generate": "node src/strategy/generate_signals.js",
    "signals:send": "node src/telegram/send_signals.js",
    "test": "node src/tests/strategy.test.js && node src/tests/telegram.test.js",
    "test:strategy": "node src/tests/strategy.test.js",
    "test:telegram": "node src/tests/telegram.test.js",
    "backtest": "node src/backtest/index.js",
    "healthcheck": "node deploy/healthcheck.js",
    "start": "node src/main.js",
    "lint": "echo 'Linting not configured yet'",
    "deploy": "bash deploy/start.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.66.0",
    "pino": "^9.14.0",
    "playwright": "^1.56.1"
  }
}
